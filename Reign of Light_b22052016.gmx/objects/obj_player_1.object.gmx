with(obj_player_stats) {

    Experience = Experience + Exp_amount;
    
    //Level up
    if (Experience&gt;=Exp_to_Levelup){
        Level = Level + 1;
        Experience = Experience - Exp_to_Levelup;
        Exp_to_Levelup = Exp_to_Levelup + Exp_Scaling;
        Exp_Scaling = round(Exp_Scaling*1.3);
        Max_Health = Max_Health + 2;
        Health = Max_Health;
        Max_Stamina = Max_Stamina + 3;
        Stamina = Max_Stamina;
        
        if(Level mod 2 == 0 ){
            Attack = Attack +1;
        }
       
    }
}

with(other) { 
    instance_destroy();
} 

</string>
